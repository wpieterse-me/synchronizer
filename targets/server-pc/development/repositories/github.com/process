#!/bin/bash

THIS_SCRIPT_DIRECTORY="$( cd -- "$( dirname -- "${BASH_SOURCE[0]}" )" &> /dev/null && pwd )"
ROOT_SCRIPT_DIRECTORY="${THIS_SCRIPT_DIRECTORY}/../../../../.."

source "${ROOT_SCRIPT_DIRECTORY}/shared"

PROCESS_ALUCRYD_SCRIPT="${THIS_SCRIPT_DIRECTORY}/alucryd/process"
PROCESS_ANDREAS_ABEL_SCRIPT="${THIS_SCRIPT_DIRECTORY}/andreas-abel/process"
PROCESS_AXBOE_SCRIPT="${THIS_SCRIPT_DIRECTORY}/axboe/process"
PROCESS_BAZELBUILD_SCRIPT="${THIS_SCRIPT_DIRECTORY}/bazelbuild/process"
PROCESS_BITS_SCRIPT="${THIS_SCRIPT_DIRECTORY}/bits/process"
PROCESS_BOOSTORG_SCRIPT="${THIS_SCRIPT_DIRECTORY}/boostorg/process"
PROCESS_CHOCOLATE_DOOM_SCRIPT="${THIS_SCRIPT_DIRECTORY}/chocolate-doom/process"
PROCESS_CMOSHER01_SCRIPT="${THIS_SCRIPT_DIRECTORY}/cmosher01/process"
PROCESS_COELCKERS_SCRIPT="${THIS_SCRIPT_DIRECTORY}/coelckers/process"
PROCESS_DHEWM3_SCRIPT="${THIS_SCRIPT_DIRECTORY}/dhewm3/process"
PROCESS_ELECTRONICARTS_SCRIPT="${THIS_SCRIPT_DIRECTORY}/electronicarts/process"
PROCESS_GOOGLE_SCRIPT="${THIS_SCRIPT_DIRECTORY}/google/process"
PROCESS_GOOGLEAPIS_SCRIPT="${THIS_SCRIPT_DIRECTORY}/googleapis/process"
PROCESS_GOOGLECLOUDPLATFORM_SCRIPT="${THIS_SCRIPT_DIRECTORY}/GoogleCloudPlatform/process"
PROCESS_ID_SOFTWARE_SCRIPT="${THIS_SCRIPT_DIRECTORY}/id-Software/process"
PROCESS_INTEL_SCRIPT="${THIS_SCRIPT_DIRECTORY}/intel/process"
PROCESS_IOQUAKE_SCRIPT="${THIS_SCRIPT_DIRECTORY}/ioquake/process"
PROCESS_KHRONOSGROUP_SCRIPT="${THIS_SCRIPT_DIRECTORY}/KhronosGroup/process"
PROCESS_LEWISSBAKER_SCRIPT="${THIS_SCRIPT_DIRECTORY}/lewissbaker/process"
PROCESS_LLVM_SCRIPT="${THIS_SCRIPT_DIRECTORY}/llvm/process"
PROCESS_MANDREYEL_SCRIPT="${THIS_SCRIPT_DIRECTORY}/mandreyel/process"
PROCESS_MESA3D_SCRIPT="${THIS_SCRIPT_DIRECTORY}/mesa3d/process"
PROCESS_MICROSOFT_SCRIPT="${THIS_SCRIPT_DIRECTORY}/microsoft/process"
PROCESS_MIST64_SCRIPT="${THIS_SCRIPT_DIRECTORY}/mist64/process"
PROCESS_MOHANSON_SCRIPT="${THIS_SCRIPT_DIRECTORY}/mohanson/process"
PROCESS_NOVUM_SCRIPT="${THIS_SCRIPT_DIRECTORY}/Novum/process"
PROCESS_ONEAPI_SRC_SCRIPT="${THIS_SCRIPT_DIRECTORY}/oneapi-src/process"
PROCESS_POCL_SCRIPT="${THIS_SCRIPT_DIRECTORY}/pocl/process"
PROCESS_PRINEXYZ_SCRIPT="${THIS_SCRIPT_DIRECTORY}/prinexyz/process"
PROCESS_PRISMARINEJS_SCRIPT="${THIS_SCRIPT_DIRECTORY}/PrismarineJS/process"
PROCESS_ROBERTBECKEBANS_SCRIPT="${THIS_SCRIPT_DIRECTORY}/RobertBeckebans/process"
PROCESS_SUIJINGFENG_SCRIPT="${THIS_SCRIPT_DIRECTORY}/suijingfeng/process"
PROCESS_SIFIVE_SCRIPT="${THIS_SCRIPT_DIRECTORY}/sifive/process"
PROCESS_TIANOCORE_SCRIPT="${THIS_SCRIPT_DIRECTORY}/tianocore/process"
PROCESS_TORRSAMAHO_SCRIPT="${THIS_SCRIPT_DIRECTORY}/TorrSamaho/process"
PROCESS_WPIETERSEXYZ_SCRIPT="${THIS_SCRIPT_DIRECTORY}/wpietersexyz/process"
PROCESS_YQUAKE2_SCRIPT="${THIS_SCRIPT_DIRECTORY}/yquake2/process"

function Process {
    ProcessCommonArguments $@
    ProcessCommonPre

    if [[ ${PARAMETER_DISABLE_REPOSITORIES} -eq 0 ]];
    then
        ProcessCallSubScripts \
            ${PROCESS_ALUCRYD_SCRIPT} \
            ${PROCESS_ANDREAS_ABEL_SCRIPT} \
            ${PROCESS_AXBOE_SCRIPT} \
            ${PROCESS_BAZELBUILD_SCRIPT} \
            ${PROCESS_BITS_SCRIPT} \
            ${PROCESS_BOOSTORG_SCRIPT} \
            ${PROCESS_CHOCOLATE_DOOM_SCRIPT} \
            ${PROCESS_CMOSHER01_SCRIPT} \
            ${PROCESS_COELCKERS_SCRIPT} \
            ${PROCESS_DHEWM3_SCRIPT} \
            ${PROCESS_ELECTRONICARTS_SCRIPT} \
            ${PROCESS_GOOGLE_SCRIPT} \
            ${PROCESS_GOOGLEAPIS_SCRIPT} \
            ${PROCESS_GOOGLECLOUDPLATFORM_SCRIPT} \
            ${PROCESS_ID_SOFTWARE_SCRIPT} \
            ${PROCESS_INTEL_SCRIPT} \
            ${PROCESS_IOQUAKE_SCRIPT} \
            ${PROCESS_KHRONOSGROUP_SCRIPT} \
            ${PROCESS_LEWISSBAKER_SCRIPT} \
            ${PROCESS_LLVM_SCRIPT} \
            ${PROCESS_MANDREYEL_SCRIPT} \
            ${PROCESS_MESA3D_SCRIPT} \
            ${PROCESS_MICROSOFT_SCRIPT} \
            ${PROCESS_MIST64_SCRIPT} \
            ${PROCESS_MOHANSON_SCRIPT} \
            ${PROCESS_NOVUM_SCRIPT} \
            ${PROCESS_ONEAPI_SRC_SCRIPT} \
            ${PROCESS_POCL_SCRIPT} \
            ${PROCESS_PRINEXYZ_SCRIPT} \
            ${PROCESS_PRISMARINEJS_SCRIPT} \
            ${PROCESS_ROBERTBECKEBANS_SCRIPT} \
            ${PROCESS_SUIJINGFENG_SCRIPT} \
            ${PROCESS_SIFIVE_SCRIPT} \
            ${PROCESS_TIANOCORE_SCRIPT} \
            ${PROCESS_TORRSAMAHO_SCRIPT} \
            ${PROCESS_WPIETERSEXYZ_SCRIPT} \
            ${PROCESS_YQUAKE2_SCRIPT}
    fi

    ProcessCommonPost
}

Process $@
