#!/bin/bash

THIS_SCRIPT_DIRECTORY="$( cd -- "$( dirname -- "${BASH_SOURCE[0]}" )" &> /dev/null && pwd )"
ROOT_SCRIPT_DIRECTORY="${THIS_SCRIPT_DIRECTORY}/../../../../.."

source "${ROOT_SCRIPT_DIRECTORY}/shared"

PROCESS_0AD_SCRIPT="${THIS_SCRIPT_DIRECTORY}/0ad/process"
PROCESS_ANDIKLEEN_SCRIPT="${THIS_SCRIPT_DIRECTORY}/andikleen/process"
PROCESS_APPLE_SCRIPT="${THIS_SCRIPT_DIRECTORY}/apple/process"
PROCESS_ARDOUR_SCRIPT="${THIS_SCRIPT_DIRECTORY}/Ardour/process"
PROCESS_AUDACITY_SCRIPT="${THIS_SCRIPT_DIRECTORY}/audacity/process"
PROCESS_AWS_SCRIPT="${THIS_SCRIPT_DIRECTORY}/aws/process"
PROCESS_AXBOE_SCRIPT="${THIS_SCRIPT_DIRECTORY}/axboe/process"
PROCESS_BAZELBUILD_SCRIPT="${THIS_SCRIPT_DIRECTORY}/bazelbuild/process"
PROCESS_BOOSTORG_SCRIPT="${THIS_SCRIPT_DIRECTORY}/boostorg/process"
PROCESS_BULLETPHYSICS_SCRIPT="${THIS_SCRIPT_DIRECTORY}/bulletphysics/process"
PROCESS_CHOCOLATE_DOOM_SCRIPT="${THIS_SCRIPT_DIRECTORY}/chocolate-doom/process"
PROCESS_CMANGOS_SCRIPT="${THIS_SCRIPT_DIRECTORY}/cmangos/process"
PROCESS_CROTEAM_OFFICIAL_SCRIPT="${THIS_SCRIPT_DIRECTORY}/Croteam-official/process"
PROCESS_CRYTEK_SCRIPT="${THIS_SCRIPT_DIRECTORY}/CRYTEK/process"
PROCESS_ELECTRONICARTS_SCRIPT="${THIS_SCRIPT_DIRECTORY}/electronicarts/process"
PROCESS_EPICGAMES_SCRIPT="${THIS_SCRIPT_DIRECTORY}/EpicGames/process"
PROCESS_FREECAD_SCRIPT="${THIS_SCRIPT_DIRECTORY}/FreeCAD/process"
PROCESS_FRICTIONALGAMES_SCRIPT="${THIS_SCRIPT_DIRECTORY}/FrictionalGames/process"
PROCESS_GOOGLE_SCRIPT="${THIS_SCRIPT_DIRECTORY}/google/process"
PROCESS_GOOGLEAPIS_SCRIPT="${THIS_SCRIPT_DIRECTORY}/googleapis/process"
PROCESS_GOOGLECLOUDPLATFORM_SCRIPT="${THIS_SCRIPT_DIRECTORY}/GoogleCloudPlatform/process"
PROCESS_HAIKU_SCRIPT="${THIS_SCRIPT_DIRECTORY}/haiku/process"
PROCESS_HRYDGARD_SCRIPT="${THIS_SCRIPT_DIRECTORY}/hrydgard/process"
PROCESS_ID_SOFTWARE_SCRIPT="${THIS_SCRIPT_DIRECTORY}/id-Software/process"
PROCESS_IHHUB_SCRIPT="${THIS_SCRIPT_DIRECTORY}/ihhub/process"
PROCESS_IOQUAKE_SCRIPT="${THIS_SCRIPT_DIRECTORY}/ioquake/process"
PROCESS_KDE_SCRIPT="${THIS_SCRIPT_DIRECTORY}/KDE/process"
PROCESS_KRANX_SCRIPT="${THIS_SCRIPT_DIRECTORY}/KranX/process"
PROCESS_LEANDROMOREIRA_SCRIPT="${THIS_SCRIPT_DIRECTORY}/leandromoreira/process"
PROCESS_LLVM_SCRIPT="${THIS_SCRIPT_DIRECTORY}/llvm/process"
PROCESS_MINETEST_SCRIPT="${THIS_SCRIPT_DIRECTORY}/minetest/process"
PROCESS_NIGHTDIVE_STUDIO_SCRIPT="${THIS_SCRIPT_DIRECTORY}/NightDive-Studio/process"
PROCESS_NOVUM_SCRIPT="${THIS_SCRIPT_DIRECTORY}/Novum/process"
PROCESS_OPENLOCO_SCRIPT="${THIS_SCRIPT_DIRECTORY}/OpenLoco/process"
PROCESS_OPENMW_SCRIPT="${THIS_SCRIPT_DIRECTORY}/OpenMW/process"
PROCESS_OPENRA_SCRIPT="${THIS_SCRIPT_DIRECTORY}/OpenRA/process"
PROCESS_OPENRCT2_SCRIPT="${THIS_SCRIPT_DIRECTORY}/OpenRCT2/process"
PROCESS_OPENTTD_SCRIPT="${THIS_SCRIPT_DIRECTORY}/OpenTTD/process"
PROCESS_OPENXRAY_SCRIPT="${THIS_SCRIPT_DIRECTORY}/OpenXRay/process"
PROCESS_POCL_SCRIPT="${THIS_SCRIPT_DIRECTORY}/pocl/process"
PROCESS_REACTOS_SCRIPT="${THIS_SCRIPT_DIRECTORY}/reactos/process"
PROCESS_REDPROJECTS_SCRIPT="${THIS_SCRIPT_DIRECTORY}/RedProjects/process"
PROCESS_ROBERTBECKEBANS_SCRIPT="${THIS_SCRIPT_DIRECTORY}/RobertBeckebans/process"
PROCESS_RUNUO_SCRIPT="${THIS_SCRIPT_DIRECTORY}/runuo/process"
PROCESS_SFTTECH_SCRIPT="${THIS_SCRIPT_DIRECTORY}/SFTtech/process"
PROCESS_SPRING_SCRIPT="${THIS_SCRIPT_DIRECTORY}/spring/process"
PROCESS_STORM_DEVS_SCRIPT="${THIS_SCRIPT_DIRECTORY}/storm-devs/process"
PROCESS_TENSORFLOW_SCRIPT="${THIS_SCRIPT_DIRECTORY}/tensorflow/process"
PROCESS_WPIETERSE2825_SCRIPT="${THIS_SCRIPT_DIRECTORY}/wpieterse2825/process"
PROCESS_YQUAKE2_SCRIPT="${THIS_SCRIPT_DIRECTORY}/yquake2/process"
PROCESS_YUZU_EMU_SCRIPT="${THIS_SCRIPT_DIRECTORY}/yuzu-emu/process"

function Process {
    ProcessCommonArguments $@
    ProcessCommonPre

    if [[ ${PARAMETER_DISABLE_REPOSITORIES} -eq 0 ]];
    then
        ProcessCallSubScripts \
            "${PROCESS_0AD_SCRIPT}" \
            "${PROCESS_ANDIKLEEN_SCRIPT}" \
            "${PROCESS_APPLE_SCRIPT}" \
            "${PROCESS_ARDOUR_SCRIPT}" \
            "${PROCESS_AUDACITY_SCRIPT}" \
            "${PROCESS_AWS_SCRIPT}" \
            "${PROCESS_AXBOE_SCRIPT}" \
            "${PROCESS_BAZELBUILD_SCRIPT}" \
            "${PROCESS_BOOSTORG_SCRIPT}" \
            "${PROCESS_BULLETPHYSICS_SCRIPT}" \
            "${PROCESS_CHOCOLATE_DOOM_SCRIPT}" \
            "${PROCESS_CMANGOS_SCRIPT}" \
            "${PROCESS_CROTEAM_OFFICIAL_SCRIPT}" \
            "${PROCESS_CRYTEK_SCRIPT}" \
            "${PROCESS_ELECTRONICARTS_SCRIPT}" \
            "${PROCESS_EPICGAMES_SCRIPT}" \
            "${PROCESS_FREECAD_SCRIPT}" \
            "${PROCESS_FRICTIONALGAMES_SCRIPT}" \
            "${PROCESS_GOOGLE_SCRIPT}" \
            "${PROCESS_GOOGLEAPIS_SCRIPT}" \
            "${PROCESS_GOOGLECLOUDPLATFORM_SCRIPT}" \
            "${PROCESS_HAIKU_SCRIPT}" \
            "${PROCESS_HRYDGARD_SCRIPT}" \
            "${PROCESS_ID_SOFTWARE_SCRIPT}" \
            "${PROCESS_IHHUB_SCRIPT}" \
            "${PROCESS_IOQUAKE_SCRIPT}" \
            "${PROCESS_KDE_SCRIPT}" \
            "${PROCESS_KRANX_SCRIPT}" \
            "${PROCESS_LEANDROMOREIRA_SCRIPT}" \
            "${PROCESS_LLVM_SCRIPT}" \
            "${PROCESS_MINETEST_SCRIPT}" \
            "${PROCESS_NIGHTDIVE_STUDIO_SCRIPT}" \
            "${PROCESS_NOVUM_SCRIPT}" \
            "${PROCESS_OPENLOCO_SCRIPT}" \
            "${PROCESS_OPENMW_SCRIPT}" \
            "${PROCESS_OPENRA_SCRIPT}" \
            "${PROCESS_OPENRCT2_SCRIPT}" \
            "${PROCESS_OPENTTD_SCRIPT}" \
            "${PROCESS_OPENXRAY_SCRIPT}" \
            "${PROCESS_POCL_SCRIPT}" \
            "${PROCESS_REACTOS_SCRIPT}" \
            "${PROCESS_REDPROJECTS_SCRIPT}" \
            "${PROCESS_ROBERTBECKEBANS_SCRIPT}" \
            "${PROCESS_RUNUO_SCRIPT}" \
            "${PROCESS_SFTTECH_SCRIPT}" \
            "${PROCESS_SPRING_SCRIPT}" \
            "${PROCESS_STORM_DEVS_SCRIPT}" \
            "${PROCESS_TENSORFLOW_SCRIPT}" \
            "${PROCESS_WPIETERSE2825_SCRIPT}" \
            "${PROCESS_YQUAKE2_SCRIPT}" \
            "${PROCESS_YUZU_EMU_SCRIPT}"
    fi

    ProcessCommonPost
}

Process $@
